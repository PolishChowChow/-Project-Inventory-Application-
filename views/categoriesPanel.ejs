<%-include('components/panel-core')%>
<main>
    <h2>Parts panel</h2>
    <table>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Delete</th>

        </tr>
        <%categories.forEach(category => { %>
                <tr>
                    <td>
                        <a href="/panel/update/category/<%=category._id%>">
                            <%=category.name%>
                        </a>
                    </td>
                    <td>
                        <%=category.description%>
                    </td>
                    <td>
                        <a href="/panel/delete/category/<%=category._id%>">Delete</a>
                    </td>
                </tr>
        <%})%>
    </table>
</main>